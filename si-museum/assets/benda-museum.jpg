    overflow: hidden;
    position: relative;
    user-over: none;
    outline: 0;
    z-index: 9
}

@keyframes  ripple {
    100% {
        -webkit-transform: scale(2.5);
        transform: scale(2.5);
        opacity: 0
    }
}

